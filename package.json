{
  "name": "clarity-types",
  "version": "0.1.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "clarity-types",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi",
        "universal-apple-darwin",
        "riscv64gc-unknown-linux-gnu"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "napi:artifacts": "napi artifacts",
    "napi:build": "napi build --platform --release -p native --cargo-name native --js native.js --dts native.d.ts",
    "napi:build:debug": "napi build --platform -p native --cargo-name native --js native.js --dts native.d.ts",
    "napi:universal": "napi universal",
    "napi:version": "napi version",
    "prepublishOnly": "napi prepublish -t npm"
  },
  "repository": "git+https://github.com/eriicafes/clarity-types.git"
}